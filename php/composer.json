{
  "name": "alibabacloud/credentials-api",
  "description": "Alibaba Cloud Credentials API Library",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Alibaba Cloud SDK",
      "email": "sdk-team@alibabacloud.com"
